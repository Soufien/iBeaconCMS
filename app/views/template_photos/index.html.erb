<h1>Listing template_photos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @template_photos.each do |template_photo| %>
      <tr>
        <td><%= link_to 'Show', template_photo %></td>
        <td><%= link_to 'Edit', edit_template_photo_path(template_photo) %></td>
        <td><%= link_to 'Destroy', template_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Template photo', new_template_photo_path %>
